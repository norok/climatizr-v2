{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "climatizr-v2": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "node_modules/font-awesome/fonts"
            ],
            "styles": [
              "src/styles.scss",
              "node_modules/font-awesome/css/font-awesome.min.css",
              "src/assets/jquery-ui/jquery-ui.min.css",
              "src/assets/jquery-ui/jquery-ui.structure.min.css",
              "src/assets/jquery-ui/jquery-ui.theme.min.css",
              "src/assets/owl-carousel/assets/owl.carousel.min.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "src/assets/jquery-ui/jquery-ui.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
              "src/assets/skycons.js",
              "src/assets/owl-carousel/owl.carousel.min.js",
              "src/assets/Chart.bundle.min.js"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "climatizr-v2:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "climatizr-v2:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "climatizr-v2:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "src/assets/jquery-ui/jquery-ui.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
              "src/assets/skycons.js",
              "src/assets/owl-carousel/owl.carousel.min.js",
              "src/assets/Chart.bundle.min.js"
            ],
            "styles": [
              "src/styles.scss",
              "node_modules/font-awesome/css/font-awesome.min.css",
              "src/assets/jquery-ui/jquery-ui.min.css",
              "src/assets/jquery-ui/jquery-ui.structure.min.css",
              "src/assets/jquery-ui/jquery-ui.theme.min.css",
              "src/assets/owl-carousel/assets/owl.carousel.min.css"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "node_modules/font-awesome/fonts"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "climatizr-v2-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "climatizr-v2:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "climatizr-v2",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "cl2",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "cl2"
    }
  }
}